{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "no-self-assign": 0,
    "no-param-reassign": 0,
    "no-return-assign": 0,
    "no-alert": 0,
    "no-continue": 0,
    "no-console": ["error", { "allow": ["warn", "error", "log"] }]
  }
}